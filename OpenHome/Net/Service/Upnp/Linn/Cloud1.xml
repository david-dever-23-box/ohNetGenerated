<?xml version="1.0" encoding="utf-8"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <actionList>
    <action>
      <name>GetChallengeResponse</name>
      <argumentList>
        <argument>
          <name>Challenge</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_String</relatedStateVariable>
        </argument>
        <argument>
          <name>Response</name>
          <direction>out</direction>
          <relatedStateVariable>A_ARG_TYPE_String</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetAssociationStatus</name>
      <argumentList>
        <argument>
          <name>Status</name>
          <direction>in</direction>
          <relatedStateVariable>AssociationStatus</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetAssociationStatus</name>
      <argumentList>
        <argument>
          <name>Status</name>
          <direction>out</direction>
          <relatedStateVariable>AssociationStatus</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetControlEnabled</name>
      <argumentList>
        <argument>
          <name>Enabled</name>
          <direction>in</direction>
          <relatedStateVariable>ControlEnabled</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetControlEnabled</name>
      <argumentList>
        <argument>
          <name>Enabled</name>
          <direction>out</direction>
          <relatedStateVariable>ControlEnabled</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
  </actionList>
  <serviceStateTable>
    <stateVariable sendEvents="yes">
      <name>AssociationStatus</name>
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>Associated</allowedValue>
        <allowedValue>NotAssociated</allowedValue>
        <allowedValue>Unconfigured</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>ControlEnabled</name>
      <dataType>boolean</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_String</name>
      <dataType>string</dataType>
    </stateVariable>
  </serviceStateTable>
</scpd>
